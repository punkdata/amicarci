        <active>true</active>
        <errorConditionFormula>AND(Simulacion__r.Opportunity.Formalizacion_OK__c,  OR(ISCHANGED(Numero_de_pago__c),ISCHANGED( Pago__c ),ISCHANGED( Fecha_vencimiento__c )))</errorConditionFormula>
